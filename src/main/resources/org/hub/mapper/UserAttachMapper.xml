<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.mapper.UserAttachMapper">

	<insert id="insert">
		insert into attach_tbl (uuid, uploadpath, filename, filename, filetype, uidkey)
		values (#{uuid},#{uploadPath},#{fileName},#{fileType},#{uidkey})
	</insert>
	
	<delete id="delete">
		delete from attach_tbl where uuid=#{uuid}
	</delete>
	
	<select id="findByUidKey" resultType="org.zerock.domain.UserAttachVO">
		select*from attach_tbl where uidkey = #{uidkey}
	</select>



</mapper>